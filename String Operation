using System;

class StringOperations
{
    static void Main()
    {
        string sample = "Hello, C#";

        Console.WriteLine("Original: " + sample);
        Console.WriteLine("Uppercase: " + sample.ToUpper());
        Console.WriteLine("Substring: " + sample.Substring(7));
        Console.WriteLine("Contains 'C#': " + sample.Contains("C#"));
        Console.WriteLine("Replaced: " + sample.Replace("C#", "World"));
    }
}
