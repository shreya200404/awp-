using System; 
namespace vowels 
{ 
class Program 
{ 
static void Main(string[] args) 
{ 
char ch; 
Console.Write("Enter a character: "); 
ch = (char)Console.Read(); 
switch(ch)
{
case 'a': 
case 'A': 
case 'e': 
case 'E': 
case 'i': 
case 'I': 
case 'o': 
case 'O': 
case 'u': 
case 'U': 
Console.WriteLine("The character is a vowel."); 
break; 
default: 
Console.WriteLine("The character is not a vowel."); 
break;
  }}}} 
